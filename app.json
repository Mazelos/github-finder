{
  "expo": {
    "name": "github-finder",
    "slug": "github-finder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "samSapi0l.githubFinder",
      "versionCode": 1
    },
    "description": "Application to find Github users",
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "githubUrl": "https://github.com/Mazelos/github-finder.git"
  }
}
